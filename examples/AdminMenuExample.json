{
  "Prefix": "{green}[Menu]{default}",
  "Menus": {
    "Admin Menu": {
      "Type": "CenterHtmlMenu",
      "Command": ["css_adminmenu"],
      "Permission": ["@css/generic"],
      "Options": {
        "Change Map": {
          "Command": ["css_adminmenumaps"]
        },
        "Shutdown Server": {
          "Command": ["quit"],
          "Confirm": true
        }
      }
    },
    "Change Map": {
      "Type": "ChatMenu",
      "Command": ["css_adminmenumaps"],
      "Permission": ["@css/generic"],
      "ExitButton": true,
      "Options": {
        "de_mirage": {
          "Command": ["map de_mirage"],
          "Confirm": true
        },
        "de_dust2": {
          "Command": ["map de_dust2"],
          "Confirm": true
        }
      }
    }
  }
}